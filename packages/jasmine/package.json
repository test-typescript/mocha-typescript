    }
  ],
  "homepage": "https://testdeck.org/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/testdeck/testdeck.git"
  },
  "main": "dist/index.js",
  "types": "index.d.ts",
  "scripts": {
